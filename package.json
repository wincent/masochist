{
  "name": "masochist",
  "version": "0.0.1",
  "description": "Masochist server",
  "main": "dist/server/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/wincent/masochist.git"
  },
  "config": {
    "content-repo": "."
  },
  "scripts": {
    "build": "gulp build",
    "check": "flow",
    "coverage": "jest --forceExit --coverage",
    "flow": "flow",
    "gulp": "gulp",
    "lint": "eslint src",
    "pretty": "babel-node scripts/pretty.js",
    "pretty-check": "babel-node scripts/pretty.js --check",
    "relay": "babel-node scripts/compile.js",
    "start": "MASOCHIST_CONTENT_REPO=$npm_package_config_content_repo babel-node src/server/main.js",
    "start-prod": "MASOCHIST_CONTENT_REPO=$npm_package_config_content_repo NODE_ENV=production node dist/server/main.js",
    "test": "jest --forceExit",
    "update-indices": "MASOCHIST_CONTENT_REPO=$npm_package_config_content_repo babel-node src/bin/updateIndices.js",
    "update-schema": "babel-node src/bin/updateSchema.js"
  },
  "author": {
    "name": "Greg Hurrell",
    "email": "greg@hurrell.net",
    "url": "https://github.com/wincent"
  },
  "license": "CC-BY-NC-SA-4.0",
  "private": true,
  "dependencies": {
    "@babel/cli": "*",
    "@babel/core": "*",
    "@babel/node": "7.0.0",
    "@babel/plugin-proposal-class-properties": "*",
    "@babel/plugin-proposal-object-rest-spread": "*",
    "@babel/polyfill": "*",
    "@babel/preset-env": "*",
    "@babel/preset-flow": "*",
    "@babel/preset-react": "*",
    "@babel/register": "*",
    "ansi-colors": "*",
    "assets-webpack-plugin": "*",
    "autoprefixer": "*",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "*",
    "babel-loader": "*",
    "babel-plugin-minify-replace": "*",
    "babel-plugin-relay": "*",
    "body-parser": "*",
    "classnames": "*",
    "css-loader": "*",
    "cssnano": "4.1.7",
    "dataloader": "*",
    "entities": "*",
    "express": "*",
    "express-graphql": "*",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "fancy-log": "*",
    "file-loader": "*",
    "flow-bin": "*",
    "flow-typed": "*",
    "fs-extra": "*",
    "glob": "^7.1.2",
    "graphql": "*",
    "graphql-relay": "*",
    "gulp": "4.0.0",
    "gulp-babel": "*",
    "highlight.js": "*",
    "history": "*",
    "isomorphic-fetch": "*",
    "markdown-it": "*",
    "memcached": "*",
    "plugin-error": "*",
    "postcss": "*",
    "postcss-loader": "*",
    "prettier": "*",
    "prop-types": "*",
    "react": "*",
    "react-dom": "*",
    "react-relay": "*",
    "redis": "*",
    "relay-compiler": "*",
    "relay-runtime": "*",
    "rss": "*",
    "scroll-behavior": "*",
    "style-loader": "*",
    "terser-webpack-plugin": "*",
    "universal-router": "*",
    "unpack-content": "*",
    "url-loader": "*",
    "webpack": "*"
  },
  "devDependencies": {
    "gulp-eslint": "*",
    "http-proxy": "*",
    "jest": "*",
    "webpack-dev-server": "*"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0",
    "graphql": "14.0.2"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "globals": {
      "__DEV__": true
    }
  }
}
